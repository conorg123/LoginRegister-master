<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.FoodForThought.loginregisterFFT">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.FoodForThought.loginregisterFFT.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.FoodForThought.loginregisterFFT.Login"
            android:label="@string/title_activity_login" />
        <activity
            android:name="com.FoodForThought.loginregisterFFT.Register"
            android:label="@string/title_activity_register" />
    </application>

</manifest>
